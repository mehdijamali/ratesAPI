{
  "name": "server",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "start": "pm2-runtime start --interpreter babel-node ./index.js",
    "stop": "pm2-runtime stop --interpreter babel-node ./index.js",
    "restart": "pm2-runtime restart --interpreter babel-node ./index.js",
    "status": "./node_modules/.bin/pm2 status",
    "monitor": "./node_modules/.bin/pm2 monit",
    "test": "cross-env NODE_ENV=test  babel-node ./node_modules/.bin/_mocha",
    "test:watch": "cross-env NODE_ENV=test nyc nodemon --exec  babel-node ./node_modules/.bin/_mocha",
    "dev": "nodemon --exec babel-node ./index.js  -e js",
    "build": "babel ./index.js --out-dir ./dist",
    "serve": "node dist/index.js"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@babel/cli": "^7.8.3",
    "@babel/core": "^7.8.3",
    "@babel/node": "^7.8.3",
    "@babel/plugin-proposal-object-rest-spread": "^7.8.3",
    "@babel/plugin-transform-runtime": "^7.11.5",
    "@babel/preset-env": "^7.8.3",
    "body-parser": "^1.19.0",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "express-validator": "^6.6.1",
    "node-fetch": "^2.6.1",
    "nodemon": "^2.0.2",
    "pg": "^8.3.3",
    "pm2": "^4.5.0",
    "webpack": "^4.41.5",
    "webpack-dev-server": "^3.10.1"
  },
  "devDependencies": {
    "chai": "^4.2.0",
    "cross-env": "^7.0.2",
    "eslint": "^6.1.0",
    "eslint-config-airbnb": "^18.0.1",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "mocha": "^8.1.3",
    "nyc": "^15.1.0",
    "supertest": "^5.0.0"
  }
}
